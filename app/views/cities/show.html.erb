<H1>Cities Page</H1>

<img class="map2" src="">

<% @cities.articles.each do |article| %>
	<div class="articles2">
    <li><%= link_to article.title, article %></li>
    <li><%= article.city %></li>
    <li><%= article.content %></li>
    <%= link_to "Edit this Article", edit_article_path(article.id) %><br>
	<%= link_to "Delete this Article v2", delete_article_path(article.id), method: :delete %><br>
    </div>
 <% end %>

 
